{
  "name": "nestcloud-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "pm2": "pm2 start pm2.config.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.1.2",
    "@grpc/proto-loader": "^0.5.6",
    "@nestcloud/boot": "^0.7.1",
    "@nestcloud/common": "^0.7.1",
    "@nestcloud/config": "^0.7.1",
    "@nestcloud/consul": "^0.7.17",
    "@nestcloud/grpc": "^0.7.1",
    "@nestcloud/http": "^0.7.1",
    "@nestcloud/loadbalance": "^0.7.1",
    "@nestcloud/logger": "^0.7.1",
    "@nestcloud/service": "^0.7.1",
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.5.1",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/microservices": "^7.6.12",
    "@nestjs/platform-express": "^7.6.12",
    "@nestjs/platform-ws": "^7.6.12",
    "@nestjs/terminus": "^7.1.0",
    "@nestjs/websockets": "^7.6.12",
    "consul": "^0.34.1",
    "grpc": "^1.24.6",
    "nestjs-redis": "^1.2.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/testing": "^7.5.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.20.3",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
